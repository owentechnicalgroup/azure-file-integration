{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "ApplicationInsights": {
    "ConnectionString": "YOUR_APPLICATION_INSIGHTS_CONNECTION_STRING",
    "EnableAdaptiveSampling": true,
    "EnablePerformanceCounterCollectionModule": true
  },
  "ServiceConfig": {
    "ReceiveFolder": "C:\\Temp\\ReceiveFolder"
  },
  "ServiceBusConfig": {
    "FullyQualifiedNamespace": "your-servicebus-namespace.servicebus.windows.net",
    "QueueName": "file-events"
  },
  "AzureAd": {
    "ClientId": "YOUR_CLIENT_ID",
    "ClientSecret": "YOUR_CLIENT_SECRET",
    "TenantId": "YOUR_TENANT_ID"
  }
}
